<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>primerIngreso</from-outcome>
            <to-view-id>/PreguntasKioskos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/InicioKiosko.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>generarClave</from-outcome>
            <to-view-id>/GenerarClave.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PreguntasKioskos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volverLogin</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/InicioKiosko.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volverLogin</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>consultarDatos</from-outcome>
            <to-view-id>/ConsultasDatosPersonas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/GenerarClave.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volverLogin</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ConsultasDatosPersonas.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volverLogin</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
